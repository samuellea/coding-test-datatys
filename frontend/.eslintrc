{
  "extends": "airbnb",
  "rules": {
    "consistent-return": [1],
    "strict": [0],
    "max-len": ["warn", 200],
    "no-underscore-dangle": [1],
    "no-console": "off",
    "react/jsx-props-no-spreading": "off",
    "jsx-a11y/label-has-associated-control": "off"
  },
  "env": {
    "browser": true
  }
}
